<ivysettings>
	<settings defaultResolver="public" />
    <resolvers>
        <chain name="public">
            <url name="com.springsource.repository.bundles.release">
                <ivy pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                <artifact pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>
            <url name="com.springsource.repository.bundles.external">
                <ivy pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                <artifact pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>
            <url name="org.sonatype.oss" m2compatible="true">
                <ivy pattern="http://oss.sonatype.org/content/repositories/snapshots/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
                <artifact pattern="http://oss.sonatype.org/content/repositories/snapshots/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
            </url>
            <url name="org.modafocas.repo" m2compatible="true">
                <ivy pattern="http://repo.modafocas.org/nexus/content/repositories/phpmaven/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                <artifact pattern="http://repo.modafocas.org/nexus/content/repositories/phpmaven/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>
            <url name="org.sonatype.repository" m2compatible="true">
                <ivy pattern="https://repository.sonatype.org/content/groups/forge/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                <artifact pattern="https://repository.sonatype.org/content/groups/forge/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>
            <url name="edu.umd.lib.maven" m2compatible="true">
                <ivy pattern="http://maven.lib.umd.edu/maven2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                <artifact pattern="http://maven.lib.umd.edu/maven2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>
            <url name="org.jboss.repository" m2compatible="true">
                <ivy pattern="https://repository.jboss.org/nexus/content/repositories/thirdparty-uploads/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                <artifact pattern="https://repository.jboss.org/nexus/content/repositories/thirdparty-uploads/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>
            <ibiblio name="ibiblio" m2compatible="true"/>
        </chain>
    </resolvers>
</ivysettings>